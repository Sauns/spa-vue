{
	"extends": ["prettier", "eslint:recommended"],
	"parserOptions": {
		"parser": "babel-eslint",
		"ecmaVersion": 6,
		"ecmaFeatures": {
			"experimentalObjectRestSpread": true
		},
		"sourceType": "module"
	},

	"env": {
		"browser": true,
		"commonjs": true,
		"node": true,
		"es6": true,
		"jquery": true
	},
	"rules": {
		"prettier/prettier": "error",
		"no-console": 2,
		"no-var": 1,
		"no-debugger": 2,
		"no-alert": 2,
		"no-extra-semi": 2,
		"CamelCase": {
			"properties": "always"
		}
	},
	"plugins": ["prettier"]
}
